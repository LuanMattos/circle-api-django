option_settings:
  aws:elasticbeanstalk:application:python
    DJANGO_SETTINGS_MODULE: setup.settings
  aws:elasticbeanstalk:container:python:
    WSGIPath: circle-api-django/setup/wsgi.py
    NumProcesses: 3
    NumThreads: 20
container_commands:
  01_migrate:
    command: "source /var/app/venv/path/to/venv/bin/activate && python3 manage.py migrate --noinput"
    leader_only: true